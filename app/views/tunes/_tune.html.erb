
var gTune = new TuneItem(
					<%= @tune.keys.first.id %>, <%= @tune.status %>, <%= @tune.parts %>,
				"<%= @tune.comments.nil? ? '' : @tune.comments.gsub(/\r\n/, '<br>').gsub(/\n/, '<br>').gsub(/\"/, '\\\"').html_safe %>", 
				"<%= @tune.entryDate %>", "<%= @tune.lastUpdate %>",
				
				[
				<% @tune.resources.each do |r| 
					priority = r.priority.nil? ? 0 : r.priority
				%>
				
					new ResourceItem(<%= r.id %>, <%= r.resource_type %>, "<%= r.title %>",  
					"<%= r.url %>", "<%= r.local_file %>", "<%= r.comments %>", "<%= r.entryDate %>", <%= priority %>, <%= r.status %>),
				
				
				<% end %>
				]
				
				);
				
gTune.otherTitles = [<%= @tune.other_titles.map{|t| "'"+t.title+"'" }.join(',') %>]

